from posts.models import Post
Post.objects.all()
Post.objects.create(title="첫 포스팅", content="어제는 어린이날 우리들 세상")
Post.objects.all()
Post.objects.all().values()
post = Post.objects.get(id=1)
post
post.title
post.title = "어린이날 다음 날에 쓰는 첫 포스팅(수정)"
post.save()
post.objects.all().values()
Post.objects.all().values()
